x <- (c(1:100000000))^(1/2)
g = Sys.time()

h = g - f
print(h)
                 
                 
                 

                 	
		start_time <- Sys.time()
		  invisible(sqrt(1:1e8)) # nie wysyÅ‚a wynikÃ³w na konsolÄ™
		end_time <- Sys.time()	
		
      end_time - start_time # sprawdÅº czas
                
